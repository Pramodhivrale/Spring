//		e.setUserName("pramod");
//		e.setOrderAmount(1000.0);
		
		
		
		Dboperation bean = config.getBean(Dboperation.class);
		
		//bean.deletData(e);
		
		//bean.addData(e);
		
		bean.deletAlldata();
		
		
	}

}
